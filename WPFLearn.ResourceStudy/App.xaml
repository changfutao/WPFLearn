<Application x:Class="WPFLearn.ResourceStudy.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFLearn.ResourceStudy"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MyDict.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WPFLearn.ResourceStudy;component/Resources/XTDict.xaml"></ResourceDictionary>
                <ResourceDictionary>
                    <Style TargetType="Button">
                        <Setter Property="Foreground" Value="DeepSkyBlue"></Setter>
                    </Style>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
